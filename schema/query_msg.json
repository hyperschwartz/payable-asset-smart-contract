{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "QueryMsg",
  "description": "A message sent to query contract config state.",
  "oneOf": [
    {
      "type": "object",
      "required": [
        "query_state"
      ],
      "properties": {
        "query_state": {
          "type": "object"
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "query_payable"
      ],
      "properties": {
        "query_payable": {
          "type": "object",
          "required": [
            "payable_uuid"
          ],
          "properties": {
            "payable_uuid": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    }
  ]
}
